#include <iostream>
using namespace std;

int main()
{
char charot[10];

    cout << "Input characters: \n";
    for (int x = 0; x < 10; x++)
    {
    	cin >> charot[x];
    	cout << "\n";
    }
    
    cout << "\nReverse Array: \n";
    for (int y = 9 ; y >= 0; y--)
    {
        cout << y + 1 << "th Letter " << charot[y] << "\n";
}
return 0;
}
